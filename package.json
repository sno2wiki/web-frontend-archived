{
  "name": "@mywiki/web-frontend",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "run-s lint:eslint",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lintfix": "run-s lintfix:eslint",
    "lintfix:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fmt": "run-s fmt:dprint",
