{
  "incremental": true,
  "typescript": {
    "lineWidth": 120,
    "indentWidth": 2,
    "semiColons": "always",
    "quoteStyle": "alwaysDouble"
  },
  "json": {},
  "markdown": {},
  "toml": {},
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,yml,yaml,toml,html,md}"],
  "excludes": ["**/node_modules", "**/*-lock.yaml"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.62.0.wasm",
    "https://plugins.dprint.dev/json-0.14.0.wasm",
    "https://plugins.dprint.dev/markdown-0.12.1.wasm",
    "https://plugins.dprint.dev/toml-0.5.3.wasm",
    "https://plugins.dprint.dev/prettier-0.2.2.exe-plugin@63b06beba3acd51e6d23379c47db342a1a24f3d88f3c52a775e2db3426124582"
  ]
}
